  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run migration && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "tsx -r tsconfig-paths/register ../node_modules/typeorm/cli.js -d src/config/orm.config.ts",
    "migration": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/orm.config.ts migration:run",
    "migration:status": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/orm.config.ts migration:show",
    "migration:rollback": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/orm.config.ts migration:revert",
    "migration:generate": "node scripts/migration migration:generate",
    "migration:create": "node scripts/migration migration:create",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.0",
